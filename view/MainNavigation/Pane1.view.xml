<mvc:View controllerName="DemoUXapp.controller.MainNavigation.Pane1"
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Tree id="Tree"
        items="{path: '/'}"
        mode="MultiSelect">
        <headerToolbar>
            <OverflowToolbar>
                <content>
                    <Title text="Repository" />
                    <ToolbarSpacer />
                </content>
            </OverflowToolbar>
        </headerToolbar>
        <infoToolbar>
            <OverflowToolbar>
                <content>
                    <Title text="Tree's info toolbar" />
                </content>
            </OverflowToolbar>
        </infoToolbar>
        <StandardTreeItem title="{text}" />
    </Tree>
</mvc:View>